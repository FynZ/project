        <dependency id="Cake.Common" version="0.29.0" exclude="Build,Analyzers" />
        <dependency id="Cake.Core" version="0.29.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Cake.Common" version="0.29.0" exclude="Build,Analyzers" />
        <dependency id="Cake.Core" version="0.29.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
