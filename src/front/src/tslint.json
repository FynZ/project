{
    "extends": "../tslint.json",
    "rules": {
          // MANUALLY MODIFIED SETTINGS
        "no-debugger": false,
        "one-line": [
            false,
            "check-open-brace",
            "check-catch", 
            "check-finally", 
            "check-else"
        ],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
