{
  "RoutePrefix": "monsters",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },

  "ElasticSearchIndexPrefix": "pocketmonsters-monsters",

  "jwtValidation": {
    "rsaPublicKeyXml": "Ressources/public-key.xml"
  },

  "spring": {
    "application": {
      "name": "monsters"
    }
  }
}
